{
    "extends": "tslint:all",
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration"
        ],
        "file-name-casing": false,
        "no-use-before-declare": false,
        "align": false,
        "no-floating-promises": false
    }
}
